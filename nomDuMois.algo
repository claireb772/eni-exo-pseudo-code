Algo nom_du_mois
#écrire le nom du mois

	#bloc de déclaration
	Variable numero_du_mois : entier <- 1
	
Début
	#saisie de l'utilisateur
	mois <- saisir("Tapez le numéro d'un mois entre 1 et 12")
	
	#début du programme
	Selon mois
		cas 1 : mois <- "Janvier"
