		cas 3 : mois <- "Mars"
		cas 4 : mois <- "Avril"
		cas 5 : mois <- "Mai"
		cas 6 : mois <- "Juin"
		cas 7 : mois <- "Juillet"
		cas 8 : mois <- "Août"		
		cas 9 : mois <- "Septembre"	
